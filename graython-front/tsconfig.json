{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env/env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.d.ts",
    "declaration.d.ts",
    "public/global.d.ts",
    "public/player"
  ],
  "compilerOptions": {
    "baseUrl": "./",
    "types": ["node"],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
